{
  "name": "tidbyt-transit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "render": "dotenv -- cross-var \"echo 'Render'; pixlet render %APP_ID%.star %CONFIG%\"",
    "push": "dotenv -- cross-var \"pixlet push --installation-id %INSTALLATION_ID% %DEVICE_ID% %APP_ID%.webp\"",
    "dev": "nodemon --watch ./sf-best-transit.star.py --watch .env --exec 'npm run render && npm run push'",
    "serve": "dotenv -- cross-var \"pixlet serve %APP_ID%.star\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cross-var": "^1.1.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "nodemon": "^2.0.20"
  }
}
